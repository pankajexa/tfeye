# Traffic Challan Backend Environment Configuration

# PostgreSQL Database Configuration
# Replace with your actual PostgreSQL connection string
DATABASE_URL=postgresql://username:password@localhost:5432/traffic_challan

# For AWS RDS PostgreSQL (example):
# DATABASE_URL=postgresql://username:password@your-db-host.region.rds.amazonaws.com:5432/traffic_challan

# AWS S3 Configuration for Image Storage
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=traffic-challan-images

# Node Environment
NODE_ENV=development

# Gemini AI API Key
GEMINI_API_KEY=your_gemini_api_key_here

# TSeChallan API Configuration
TSECHALLAN_API_KEY=your_tsechallan_api_key
TSECHALLAN_API_URL=https://echallan.tspolice.gov.in/TSeChallanRST

# Server Configuration
PORT=3001

# Example Production Configuration:
# NODE_ENV=production
# DATABASE_URL=postgresql://username:password@prod-db-host:5432/traffic_challan
# PORT=3001
